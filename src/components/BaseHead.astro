---
import "../styles/global.css";
import { AstroSeo } from "@astrolib/seo";
import Favicons from "./global/Favicons.astro";
---

<AstroSeo
  title="ZevoStudio"
  description="We create websites that convert visitors into customers Design and development subscriptions for everyone. Pause or cancel anytime."
  canonical="https://zevostudio.netlify.app"
  openGraph={{
    url: "https://zevostudio.netlify.app",
    title: "ZevoStudio",
    description:
      "We create websites that convert visitors into customers Design and development subscriptions for everyone. Pause or cancel anytime.",
    images: [
      {
        url: "https://zevostudio.netlify.app/opengraph/facebook.png",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "https://zevostudio.netlify.app/opengraph/twitter.png",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "https://zevostudio.netlify.app/opengraph/facebook.png" },
      { url: "https://zevostudio.netlify.app/opengraph/twitter.png" },
    ],
    site_name: "ZevoStudio",
  }}
  twitter={{
    handle: "@Mike_Andreuzza",
    site: "https://zevostudio.netlify.app",
    cardType: "summary_large_image",
  }}
/>
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width" />
<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<meta name="author" content="Azril Adya" />
<meta name="your keywords" content="Add your keywords here, web development, design, SEO, digital marketing, ZevoStudio" />

<Favicons />
<link rel="preconnect" href="https://rsms.me/" />
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />

<script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>
